---
const { title, text } = Astro.props;
import QuietCard from "../../components/quiet/QuietCard.astro";
import QuietStack from "../../components/quiet/QuietStack.astro";
import QuietTitle from "../../components/quiet/QuietTitle.astro";
import QuietText from "../../components/quiet/QuietText.astro";
---

<QuietCard>
  <QuietStack space="xs">
    <QuietTitle level={3}>{title}</QuietTitle>
    <QuietText>{text}</QuietText>
  </QuietStack>
</QuietCard>